<h1>All Tags</h1>

<ul id="articles">
  <% @tag.each do |tag| %>
    <li>
      
      <%= link_to tag.name, tag_path(tag) %>
      <%=link_to "Delete Tag", tag_path(tag), method: :delete, data: {confirm: "Really delete tag?"} %>
    </li>
  <% end %>
<%= link_to "<< Back to Articles List", articles_path %>
</ul>


